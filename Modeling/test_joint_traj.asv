t  = 60;

[th1, th2] = initial_condition_convert(2*pi/3, 0.18);
mid_l = 0.05;
thd_1_array = 0;
thd_2_array = 0;
for i = 1:t
    temp = flight_trajectory([th1, th2],mid_l,i/t);
    thd_1_array(i) = temp(1,1);
    thd_2_array(i) = temp(2,1);
end
t = linspace(0,0.6,60);
plot(t,thd_2_array)
hold on;
plot(t,thd_1_array);

p = parameters();  
p = [p; ground_height];
z = vertcat(thd_1_array, thd_2_array);
for i =1:8
    size(z)
    size( zeros(1,60))
    z = vertcat(z, zeros(1,60));
end
foot = position_foot(z,p);
plot(foot(1,:), foot(2,:));